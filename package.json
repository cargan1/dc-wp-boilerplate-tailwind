{
  "name": "dc-wp-boilerplate",
  "version": "1.0.0",
  "description": "Wordpress boilerplate",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "clean": "rm dist/*",
    "watch": "parcel watch src/main.js",
    "build": "parcel build src/main.js",
    "stylelint": "stylelint \"src/sass/**/*.scss\" --syntax scss",
    "eslint": "eslint src/js/modules",
    "lint": "npm run stylelint && npm run eslint",
    "stylelint-check": "stylelint-config-prettier-check"
  },
  "keywords": [],
  "author": {
    "name": "Daniel Cargan",
    "url": "https://cargan.dk"
  },
  "license": "ISC",
  "devDependencies": {
    "@parcel/transformer-sass": "^2.2.1",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "parcel": "^2.2.1",
    "prettier": "^2.5.1",
    "stylelint": "^14.3.0",
    "stylelint-config-prettier": "^9.0.3"
  },
  "dependencies": {
    "@barba/core": "^2.9.7",
    "lazysizes": "^5.3.2"
  }
}
